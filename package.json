{
  "name": "vacancy-frontend",
  "version": "0.1.0",
  "private": false,
  "dependencies": {
    "@emotion/core": "^10.0.14",
    "axios": "^0.19.0",
    "date-fns": "^1.30.1",
    "informed": "^2.11.8",
    "lodash": "^4.17.13",
    "normalize.css": "^8.0.1",
    "polished": "^3.4.1",
    "react": "^16.8.6",
    "react-day-picker": "^7.3.0",
    "react-dom": "^16.8.6",
    "react-modal": "^3.8.2",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "react-table": "^6.10.0",
    "typography": "^0.16.19",
    "unstated": "^2.1.1",
    "unstated-logger": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint": "eslint src/**/*js  --ignore-pattern node_modules/",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "coverage": "CI=true yarn run test --coverage && cat coverage/lcov.info | coveralls"
  },
  "eslintConfig": {
    "extends": "react-app",
    "env": {
      "jest": true
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && CI=true npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@types/jest": "^24.0.15",
    "babel-jest": "^24.8.0",
    "coveralls": "^3.0.4",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "http-proxy-middleware": "^0.19.1",
    "husky": "^3.0.0",
    "mockdate": "^2.0.3",
    "moxios": "^0.4.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "standard-version": "^6.0.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/src/**/*.js",
      "!**/setupProxy.js",
      "!**/serviceWorker.js",
      "!src/index.js",
      "!**/node_modules/**",
      "!**/vendor/**"
    ]
  }
}
